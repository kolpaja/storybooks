<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <link
      rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css'
    />
    <link
      rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css'
    />
    <link rel='stylesheet' href='/css/style.css' />
    <title>StoryBooks</title>
  </head>
  <body>
    {{> _header}}
    {{> _add_btn}}
    <div class="container">
      {{{body}}}
    </div>
    <script
      src='https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js'
    ></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/ckeditor5/35.0.1/ckeditor.min.js" integrity="sha512-xl37Mj+pZ/xUR6DjOTjqlnN7Mb60Od4efkgdfwwhlhvb2qrsB6FrcP1adt2AjiCaKe2eKkfZ0welNw5Kin4UsQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script>
      M.Sidenav.init(document.querySelector('.sidenav'))
      M.FormSelect.init(document.querySelector('#status'))
       ClassicEditor
        .create( document.querySelector( '#body' ) ,{
           toolbar: [ 'heading', '|', 'bold', 'italic', 'link', 'bulletedList', 'numberedList', 'blockQuote' ],
        })
        .catch( error => {
            console.error( error );
        } );
    </script>
  </body>
</html>